    <PackageReference Include="Fusi.DbManager.PgSql" Version="1.0.9" />
    <PackageReference Include="Fusi.Microsoft.Extensions.Configuration.InMemoryJson" Version="1.0.3" />
    <PackageReference Include="Polly" Version="7.2.3" />
    <PackageReference Include="Serilog" Version="2.10.0" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\Pythia.Api.Models\Pythia.Api.Models.csproj" />
    <ProjectReference Include="..\Pythia.Cli.Core\Pythia.Cli.Core.csproj" />
    <ProjectReference Include="..\Pythia.Core.Plugin\Pythia.Core.Plugin.csproj" />
    <ProjectReference Include="..\Pythia.Sql.PgSql\Pythia.Sql.PgSql.csproj" />
  </ItemGroup>
</Project>
